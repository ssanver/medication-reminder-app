{
  "expo": {
    "name": "Medication Reminder",
    "slug": "medication-reminder",
    "scheme": "medication-reminder",
    "version": "0.1.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "splash": {
      "backgroundColor": "#1256DB"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.medication-reminder"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#1256DB"
      }
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-apple-authentication",
      "expo-notifications"
    ]
  }
}
